<div class="container">
  <%= simple_form_for @event do |f| %>
    <%= f.input :private_event, as: :boolean, include_hidden: true %>
    <%= f.input :category, collection: ["Rehearsal", "Gig"] %>
    <%= f.input :photo, as: :file%>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :description, as: :text %>
    <%= f.submit %>
  <% end %>
</div>
